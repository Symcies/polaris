		<field name="start" type="int"/>
		<field name="type" type="int"/>
		<field name="severity" type="int"/>
		<field name="description" type="std::string"/>
		<field name="congestion" type="std::string"/>
		<field name="lane" type="std::string"/>
		<field name="lat" type="double"/>
		<field name="lng" type="double"/>
		<field name="x" type="double"/>
		<field name="y" type="double"/>
		<field name="roadClosed" type="bool"/>
