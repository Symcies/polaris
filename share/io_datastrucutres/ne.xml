<?xml version="1.0" ?>
<root>
	<type name="Network_Event" pragma="object">
		<field name="id" type="int" pragma="id"/>
		<field name="name" type="std::string"/>
		<field name="icon" type="std::string"/>
		<field name="instances" type="std::vector&lt;weak_ptr&lt;Event_Instance&gt; &gt;" pragma="inverse(event)"/>
		<field name="keys" type="std::vector&lt;shared_ptr&lt;Event_Key&gt; &gt;"/>
	</type>
	<type name="Event_Key" pragma="object">
		<field pragma="id" name="key" type="std::string"/>>
		<field name="value_type" type="std::string"/>
		<field name="value_constraint" type="std::string"/>
		<field name="required" type="bool"/>
		<field name="note" type="std::string"/>
	</type>
	<type name="Event_Instance" pragma="object">
		<field name="id" type="int" pragma="id auto"/>
		<field name="event" type="shared_ptr&lt;Network_Event&gt;" pragma="not_null"/>
		<field name="values" type="std::vector&lt;shared_ptr&lt;Event_Instance_Value&gt; &gt;"/>
		<field name="links" type="std::vector&lt;int&gt;"/>
		<field name="location_x" type="float"/>
		<field name="location_y" type="float"/>		
		<field name="reporter" type="std::string"/>					
		<field name="confidence_level" type="std::string"/>			
		<field name="time_detected" type="int"/>			
		<field name="expected_duraition" type="int"/>			
		<field name="expired" type="bool"/>			
		<field name="start_time" type="int"/>			
		<field name="end_time" type="int"/>			
		<field name="note" type="std::string"/>			
	</type>	
	<type name="Event_Instance_Value" pragma="object">
		<field name="id" type="int" pragma="id auto"/>
		<field name="key" type="shared_ptr&lt;Event_Key&gt;"/>
		<field name="value" type="std::string"/>
	</type>				
</root>